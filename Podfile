source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'

inhibit_all_warnings!

use_frameworks!


target 'FacebookAuth' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FacebookAuth

  # Facebook SDK
   pod 'FBSDKCoreKit'
   pod 'FBSDKLoginKit'
   pod 'FacebookCore'
   pod 'FacebookLogin'

end
